{
  "name": "bluelist",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "npx nodemon --watch dist --ext js,html,css --exec 'node server.js' & livereload dist --port 35730 --wait 200",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@atproto/api": "^0.13.27",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "connect-livereload": "^0.6.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-define-config": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "livereload": "^0.9.3",
    "prettier": "^3.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "express": "^4.21.2"
  }
